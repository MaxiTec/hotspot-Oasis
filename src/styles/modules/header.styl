.header
	// background: teal
	// background $cancunColor linear-gradient(180deg, rgba(248,119,119,0.35) 0%, rgba(0,0,0,0.04) 100%)
	position absolute
	left:0
	top:0
	width:100%
	// height calc(180px- 60px)
	&__path
		// height 180px
		position absolute
		width: 100%
		// top:100%
		&__overlay
			position absolute
			top:50%
			left:50%
			// max-width:100px
			z-index 300
			transform:translate(-50%,-50%)
			text-align: center
			img
				max-width:100%
				+from-width(600px)
					max-width 230px
				+from-width(5)
					max-width:260px
				+from-width(6)
					max-width:320px
			.icono
				display block
				text-align: center
				max-width 30px
				margin:0 auto
				margin-top: 15px
				img
					max-width 100%
		&__inner
			position absolute
			top:0
			left :0
			width:100%
			height 180px
			// background: url('../img/header-goc.png') no-repeat
			background-size: cover!important
			background-position: bottom center!important
			+landscape()
				height 200px
			+from-width(6)
				height 270px
